{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      { "src": "/api/videos/(.*)", "dest": "/api/index.js" },
      { "src": "/(.*)", "dest": "/public/index.html" }
    ]
  }
  